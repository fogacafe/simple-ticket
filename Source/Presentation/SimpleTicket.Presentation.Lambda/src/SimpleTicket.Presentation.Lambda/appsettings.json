{
  "Logging": {
    "LogLevel": {
      "Default": "Information", // Nível mínimo de log da aplicação
      "Microsoft": "None", // Não logar nada da Microsoft
      "Microsoft.Hosting.Lifetime": "Information" // Ajuste específico para logs de inicialização do ASP.NET
    }
  },
  "AllowedHosts": "*",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning"
      }
    }
  }
}
